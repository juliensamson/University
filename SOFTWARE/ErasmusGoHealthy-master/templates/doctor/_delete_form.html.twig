<form method="post" action="{{ path('doctor_delete', {'doctorid': doctor.doctorid}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ doctor.doctorid) }}">
    <button class="btn">Delete</button>
</form>